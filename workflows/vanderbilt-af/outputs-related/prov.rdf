@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix hca: <http://data.humancellatlas.org/> .
@prefix hm-data: <http://data.hubmapconsortium.org/> .

hm-data:00-donor-biomaterial
  a prov:Entity ;
  a hca:Biomaterial .
hm-data:00-clinical-file
  a prov:Entity ;
  a hca:File ;
  prov:wasDerivedFrom hm-data:00-donor-biomaterial .

hm-data:00-extraction-process
  a prov:Activity ;
  a hca:Process ;
  prov:used hm-data:00-extraction-protocol ;
  prov:used hm-data:00-donor-biomaterial .
hm-data:00-extraction-protocol
  a prov:Entity ;
  a hca:Protocol .
hm-data:00-organ-biomaterial
  a prov:Entity ;
  a hca:Entity ;
  prov:wasDerivedFrom hm-data:00-donor-biomaterial ;
  prov:wasGeneratedBy hm-data:00-extraction-process .
